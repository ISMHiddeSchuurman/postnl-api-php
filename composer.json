{
    "name": "firstred/postnl-api-php",
    "description": "PostNL REST API PHP Bindings",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Dekker",
            "email": "git@michaeldekker.nl"
        }
    ],
    "scripts": {
        "phpcs": "vendor/bin/php-cs-fixer fix --config=.php_cs.dist --dry-run src tests",
        "phpcs:src": "vendor/bin/php-cs-fixer fix --config=.php_cs.dist --dry-run src",
        "phpcs:tests": "vendor/bin/php-cs-fixer fix --config=.php_cs.dist --dry-run tests",
        "phpcs:fix": "vendor/bin/php-cs-fixer fix --config=.php_cs.dist src tests",
        "phpcs:fix:src": "vendor/bin/php-cs-fixer fix --config=.php_cs.dist src",
        "phpcs:fix:tests": "vendor/bin/php-cs-fixer fix  --config=.php_cs.dist tests",
        "test": "vendor/bin/phpunit --testdox tests"
    },
    "require": {
        "php": "^5.6 || ^7.0 || ^8.0",
        "ext-json": "*",
        "guzzlehttp/psr7": "^1.7 || ^2.0",
        "psr/cache": "^1.0 || ^2.0 || ^3.0",
        "psr/log": "^1.0",
        "setasign/fpdf": "^1.8",
        "setasign/fpdi": "^2.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "cache/void-adapter": "^1.0",
        "friendsofphp/php-cs-fixer": "^2.16",
        "guzzlehttp/guzzle": "^6.5.3 || ^7.0",
        "guzzlehttp/promises": "^1.4",
        "php-http/mock-client": "^1.3",
        "phpunit/phpunit": ">=5.0",
        "sabre/xml": "^1.5",
        "symfony/dotenv": "^3.0 || ^4.0 || ^5.0",
        "wappr/logger": "^1.2",
        "giggsey/libphonenumber-for-php": "^8.12"
    },
    "suggest": {
        "sabre/xml": "To use the SOAP API -- supports ^1.5.0",
        "guzzlehttp/guzzle": "One of the most popular HTTP clients available -- supports ^6.5.3 || ^7.0",
        "giggsey/libphonenumber-for-php": "To automatically format and check phone numbers -- supports ^8.12",
        "php-http/discovery": "Required if you want to use an alternative PSR-18, async or HTTPlug Client",
        "php-http/httplug": "Required if you want to use an alternative PSR-18, async or HTTPlug Client",
        "php-http/message-factory": "Required if you want to use an alternative PSR-18, async or HTTPlug Client",
        "psr/http-factory": "Required if you want to use an alternative PSR-18, async or HTTPlug Client"
    },
    "autoload": {
        "psr-4": {
            "ThirtyBees\\PostNL\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ThirtyBees\\PostNL\\Tests\\": "tests/"
        }
    }
}
